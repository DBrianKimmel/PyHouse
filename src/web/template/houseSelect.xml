<h1>PyHouse House Selection</h1>
<h2>Select the House:</h2>
<form name = 'menuofbuttons' enctype = 'multipart/form-data' method = 'post' >
	<nevow:attr name = "action">
        <nevow:invisible nevow:render = 'action' />
    </nevow:attr>
	<table style = 'Width: 100%' border = '0'>
		<tr>
			<div nevow:data="houselist" nevow:render="houselist" />
		</tr>
	</table>
	<input type = 'submit' value = 'Cancel' name = 'post_btn' />
</form>
 
