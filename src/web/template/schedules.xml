<!-- schedules.xml -->
<html   xmlns:nevow = "http://nevow.com/ns/nevow/0.1"
		xmlns = 'http://www.w3.org/1999/xhtml'
		lang = 'en'>
	<head>
		<title>PyHouse House Schedule Page</title>
		<link rel = 'stylesheet' type = 'text/css' href = 'mainPage.css' />
		<script type = 'text/javascript' src = 'ajax.js' />
		<script type = 'text/javascript' src = 'floatingWindow.js' />
		
		<script type = 'text/javascript' src = 'schedulePage.js' />
    </head>
    <body>
    	<h1>PyHouse Schedule Selection</h1>
    	<h2>Select the Schedule to modify:</h2>
    	<form name = 'menuofbuttons' enctype = 'multipart/form-data' method = 'post' >
    				<nevow:attr name = "action"><nevow:invisible nevow:render = 'action' /></nevow:attr>
    		<table style = 'Width: 100%' border = '0'>
    			<tr>
   					<div nevow:data="scheduleslist" nevow:render="scheduleslist" />
    			</tr>
    		</table>
     		<input type = 'submit' value = 'Add' name = 'post_btn' />
    		<input type = 'submit' value = 'Cancel' name = 'post_btn' />
    		<input type = 'submit' value = 'Back' name = 'post_btn' />
    	</form> 
    </body>
</html>

