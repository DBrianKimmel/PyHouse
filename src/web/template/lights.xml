<!-- lights.xml
 -->
<html
    xmlns:nevow = "http://nevow.com/ns/nevow/0.1"
	xmlns = 'http://www.w3.org/1999/xhtml'
	lang = 'en'
	>
	<head nevow:render = 'liveglue'>
		<title>PyHouse House Lights Page</title>
		<link rel = 'stylesheet' type = 'text/css' href = 'mainPage.css' />
		<script type = 'text/javascript' src = 'floatingWindow.js' />
		<script type = 'text/javascript' src = 'lightPage.js' />
    </head>
    <body>
    	<h1>PyHouse Lights Selection</h1>
    	<h2>Select the light to modify or click the add button:</h2>
    	<form name = 'menuofbuttons' enctype = 'multipart/form-data' method = 'post' >
    		<nevow:attr name = "action"><nevow:invisible nevow:render = 'action' /></nevow:attr>
    		<table style = 'Width: 100%' border = '2'>
    			<tr><td>abc</td>
    			</tr><tr>
   					<span nevow:data="lightslist" nevow:render="lightslist" />
    			</tr>
    		</table>
     		<input type = 'submit' value = 'Add New Light' name = 'post_btn' />
    		<input type = 'submit' value = 'Cancel' name = 'post_btn' />
    		<input type = 'submit' value = 'Back' name = 'post_btn' />
    	</form> 
    </body>
</html>
